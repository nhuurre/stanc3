  int<lower=occ_obs, upper=R> occ_fs;
  real psi_nd;
  // prob occurred given not detected
  psi_nd = (psi * (1 - p) ^ T) / (psi * (1 - p) ^ T + (1 - psi));
  occ_fs = occ_obs + binomial_rng(R - occ_obs, psi_nd);
}

